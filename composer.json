{
  "name":"oneup/contao-page-teaser",
  "description":"Page teaser functionality for Contao Open Source CMS",
  "keywords":["contao", "page teaser", "teaser", "preview", "extensions", "modules"],
  "type":"contao-module",
  "homepage":"http://1up.io",
  "license":"MIT",
  "authors":[
    {
      "name":"David Greminger",
      "email":"dg@1up.io",
      "homepage":"http://1up.io",
      "role":"Developer"
    },
    {
      "name":"Jim Schmid",
      "email":"js@1up.io",
      "homepage":"http://1up.io",
      "role":"Developer"
    }
  ],
  "support":{
    "email":"hello@1up.io",
    "issues":"https://github.com/1up-lab/contao-page-teaser/issues",
    "irc":"irc://irc.freenode.org/contao.dev",
    "source":"https://github.com/1up-lab/contao-page-teaser"
  },
  "require":{
    "php":">=5.4",
    "contao/core-bundle":"^3.2 || ^4.1",
    "contao-community-alliance/composer-plugin":"~2.4 | ~3.0"
  },
  "conflict": {
    "contao/core": "3.4.0"
  },
  "autoload": {
    "psr-4": {
      "Oneup\\PageTeaser\\": "src"
    }
  },
  "extra": {
    "contao": {
      "sources": {
        "contao": "system/modules/page-teaser"
      }
    }
  },
  "prefer-stable": true
}
